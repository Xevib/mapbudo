<android.support.v4.widget.DrawerLayout android:id="@+id/drawer_layout"
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:layout_gravity="center_vertical">


<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.maper.MainActivity"
    tools:ignore="MergeRootFrame" />

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frame_values"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:visibility="invisible"
    android:layout_alignParentBottom="true"
    tools:context="com.example.maper.MainActivity"
    tools:ignore="MergeRootFrame"
    
    />

<ExpandableListView android:id="@+id/left_drawer"
	android:layout_width="240dp"
	android:layout_height="match_parent"
	android:layout_gravity="start"
	android:choiceMode="singleChoice"
	android:divider="@android:color/transparent"
	android:dividerHeight="0dp"
	android:background="#111"/>

</android.support.v4.widget.DrawerLayout><!-- From: file:/home/xevi/studio_workspace/Mapbudo/app/src/main/res/layout/activity_main.xml -->